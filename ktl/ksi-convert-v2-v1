#!/usr/bin/python
from __future__ import print_function

import os
import sys
import yaml

from ubuntu import convert_v2_to_v1

# Load up the existing data.
with open("../info/kernel-series.yaml") as dfd:
    data = yaml.load(dfd)

print(
    """
#
# AUTOGENERATED FROM info/kernel-series.yaml DO NOT EDIT
#
""".lstrip()
)
data_v1 = convert_v2_to_v1(data)

yaml.dump(data_v1, sys.stdout, default_flow_style=False)
